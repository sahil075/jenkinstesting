    agent any

    stages {
        stage('Test') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Deploy on Test') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Deploy on prod') {
            steps {
                echo 'Hello World'
            }
        }
    }
}
